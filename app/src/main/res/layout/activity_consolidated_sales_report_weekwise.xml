<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Settings.ConsolidatedSalesReportWeekwise">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">



        <TextView
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:textColor="@color/TMC_White"
            android:textSize="16sp"
            android:paddingStart="20dp"
            android:paddingEnd="20dp"
            android:gravity="center_vertical"
            android:fontFamily="@font/open_sans_bold"
            android:background="@color/TMC_Orange"
            android:text="@string/Consolidated_Sales_Report_Weekwise" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"

                android:gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView4"
                    android:visibility="gone"

                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@mipmap/back_arrow" />

                <TextView
                    android:id="@+id/vendorName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight=".1"
                    android:textColor="@color/TMC_Primary_Text_Color"
                    android:textSize="15sp"
                    android:layout_marginStart="13dp"
                    android:fontFamily="@font/open_sans_bold"

                    android:text="TMC Hastinapuram" />
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="2"
                    android:orientation="horizontal">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"

                        android:textColor="@color/TMC_Orange"
                        android:textSize="15sp"
                        android:layout_marginStart="13dp"
                        android:fontFamily="@font/open_sans_bold"
                        android:gravity="end"
                        android:text="Total Sales Amount  : " />

                    <TextView
                        android:id="@+id/totalRating_headingText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/TMC_Orange"
                        android:textSize="14sp"
                        android:layout_marginStart="13dp"
                        android:fontFamily="@font/open_sans_bold"

                        android:text="00000.00" />

                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/divider"
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@drawable/border_line" />



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/dateSelectorLayout"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.2"
                    android:orientation="vertical">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="3dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/open_sans_semibold"
                        android:text="Start Date"
                        android:gravity="center_vertical"
                        android:textColor="@color/TMC_Primary_Text_Color"
                        android:textSize="12sp" />


                    <TextView
                        android:id="@+id/dateSelector_text"
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:layout_marginStart="3dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/open_sans_bold"
                        android:text="10 Jan 2021"
                        android:textColor="@color/TMC_Primary_Text_Color"
                        android:textSize="15sp" />
                </LinearLayout>




                <LinearLayout
                    android:id="@+id/endDateSelectorLayout"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.2"
                    android:orientation="vertical">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="6dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/open_sans_semibold"
                        android:text="End Date"
                        android:gravity="center_vertical"
                        android:textColor="@color/TMC_Primary_Text_Color"
                        android:textSize="12sp" />


                    <TextView
                        android:id="@+id/endDateSelector_text"
                        android:layout_width="wrap_content"
                        android:layout_height="18dp"
                        android:layout_marginStart="6dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/open_sans_bold"
                        android:text="10 Jan 2021"
                        android:textColor="@color/TMC_Primary_Text_Color"
                        android:textSize="15sp" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="right"
                    android:id="@+id/getData"
                    android:orientation="vertical">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:textColor="@color/TMC_Orange"
                        android:textSize="16sp"
                        android:fontFamily="@font/open_sans_semibold"
                        android:gravity="center_vertical"
                        android:layout_marginStart="3dp"
                        android:paddingTop="10dp"
                        android:layout_marginEnd="3dp"
                        android:text="Get Data" />

                </LinearLayout>


            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@drawable/border_line" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="5"

                    android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:textColor="@color/TMC_Primary_Text_Color"
                    android:textSize="16sp"
                    android:fontFamily="@font/open_sans_semibold"
                    android:gravity="center_vertical"
                    android:layout_marginStart="13dp"
                    android:text="Items Report" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:id="@+id/generateReport_Layout"
                    android:orientation="vertical">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:textColor="@color/TMC_Orange"
                        android:textSize="16sp"
                        android:fontFamily="@font/open_sans_semibold"
                        android:gravity="center_vertical"
                        android:layout_marginStart="13dp"
                        android:layout_marginEnd="13dp"
                        android:text="Generate Sheet" />

                </LinearLayout>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="@drawable/border_line" />

            <ScrollView
