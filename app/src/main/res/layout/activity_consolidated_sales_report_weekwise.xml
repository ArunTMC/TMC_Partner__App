                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans"
                                android:gravity="right"
                                android:text="0.00"

                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="15sp" />

                        </LinearLayout>
                        <View
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:background="@drawable/border_line" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="(E) Delivery Charge  "
                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                            <TextView
                                android:id="@+id/deliveryChargeAmount_textwidget"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans"
                                android:gravity="right"
                                android:text="0.00"

                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="15sp" />

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:background="@drawable/border_line" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:gravity="center_vertical"

                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="(F) TOTAL (A-(B+C+D+E))  "
                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                            <TextView
                                android:id="@+id/totalAmt_with_CouponDiscount"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans"
                                android:gravity="right"
                                android:text="0000.00"

                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="15sp" />

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:background="@drawable/border_line" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="(E) GST  "
                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                            <TextView
                                android:id="@+id/totalGST_Amt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans"
                                android:gravity="right"
                                android:text="0000.00"

                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="15sp" />

                        </LinearLayout>


                        <View
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:background="@drawable/border_line" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="(F) FINAL SALES (D+E)  "
                                android:textColor="@color/TMC_Orange"
                                android:textSize="16sp" />


                            <TextView
                                android:id="@+id/final_sales"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans"
                                android:gravity="right"
                                android:text="0000.00"

                                android:textColor="@color/TMC_Orange"
                                android:textSize="15sp" />

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:background="@drawable/border_line" />


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="(G) FINAL SALES BREAKUP  "
                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="1.APP SALES "
                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                            <TextView
                                android:id="@+id/appSales"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:gravity="right"
                                android:text="0000.00"
                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="2.POS SALES"
                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                            <TextView
                                android:id="@+id/posSales"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:gravity="right"
                                android:text="0.00"

                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="3.PHONE ORDER SALES"
                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                            <TextView
                                android:id="@+id/phoneOrderSales"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:gravity="right"
                                android:text="0.00"

                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:gravity="center_vertical"
                            android:visibility="visible"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="4.WHOLESALE ORDER SALES"
                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                            <TextView
                                android:id="@+id/wholesalesOrderSales"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:gravity="right"
                                android:text="0.00"

                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:gravity="center_vertical"
                            android:visibility="visible"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="5.SWIGGY SALES"
                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                            <TextView
                                android:id="@+id/swiggySales"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:gravity="right"
                                android:text="0.00"

                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                        </LinearLayout>




                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:gravity="center_vertical"
                            android:visibility="visible"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="6.DUNZO SALES"
                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                            <TextView
                                android:id="@+id/dunzoSales"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:gravity="right"
                                android:text="0.00"

                                android:textColor="@color/TMC_Primary_Text_Color"
                                android:textSize="16sp" />


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="30dp"
                            android:gravity="center_vertical"
                            android:visibility="visible"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:fontFamily="@font/open_sans_bold"
                                android:text="7.BigBasket SALES"
